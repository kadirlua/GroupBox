<ActionTemplates>
   <Action>
      <Name>GroupBox.SetLabelText</Name>
      <Description>Sets the text of a groupbox object.</Description>
      <ReturnValueType></ReturnValueType>
      <Arguments>
         <Arg>
            <Name>ObjectName</Name>
            <Description>The name of the object.</Description>
            <Type>string</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default></Default>
               <DataType>pluginobject</DataType>
               <Constraints>IR_GROUPBOX_OBJECT</Constraints>
            </EasyMode>
         </Arg>
		 <Arg>
            <Name>Text</Name>
            <Description>The text of the object.</Description>
            <Type>string</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default>"Label Text"</Default>
               <DataType>string</DataType>
               <Constraints>none</Constraints>
            </EasyMode>
         </Arg>
      </Arguments>
   </Action>
